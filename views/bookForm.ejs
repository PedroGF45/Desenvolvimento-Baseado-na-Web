<!DOCTYPE html>
<html lang="en">
  <%- include('partials/head') %>
  <body>
    <div class="container">
      <%- include('partials/header') %>
      <form action="/bookform" method="POST">
        <div class="form-group">
          <label for="">Titulo</label>
          <input type="text" class="form-control" id="" name="titulo"/>
        </div>
        <div class="form-group">
          <label for="">Autor</label>
          <input type="text" class="form-control" id="" name="autor"/>
        </div>
        <div class="form-group">
          <label for="">Texto</label>
          <input type="text" class="form-control" id="" name="text"/>
        </div>
        <div class="form-group">
          <label for="">Imagem</label>
          <input type="text" class="form-control" id="" name="img"/>
        </div>
        <button type="submit" class="btn btn-primary">Submit</button>
      </form>
    </div>
    <%- include('partials/footer') %>
    <script
      src="https://cdn.jsdelivr.net/npm/jquery@3.5.1/dist/jquery.slim.min.js"
      integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj"
      crossorigin="anonymous"
    ></script>
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-Fy6S3B9q64WdZWQUiU+q4/2Lc9npb8tCaSX9FK7E8HnRr0Jz8D6OP9dO5Vg3Q9ct"
      crossorigin="anonymous"
    ></script>
  </body>
</html>
